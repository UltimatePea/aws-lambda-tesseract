{
  "name": "@shelf/aws-lambda-tesseract",
  "version": "3.0.0",
  "description": "6 MB Tesseract 4.1 (with English training data) to fit inside AWS Lambda",
  "license": "MIT",
  "repository": "shelfio/aws-lambda-tesseract",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "test": "TZ=UTC jest src"
  },
  "files": [
    "src",
    "bin/tt.tar.gz"
  ],
  "main": "src/index.js",
  "keywords": [
    "lambda",
    "ocr",
    "tesseract"
  ],
  "dependencies": {
    "is-image": "3.0.0",
    "tar": "4.4.13"
  },
  "devDependencies": {
    "@shelf/eslint-config": "0.13.0",
    "@types/node": "12",
    "eslint": "6.8.0",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
